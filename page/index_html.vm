<!doctype html>
<html lang="en" ng-app="myApp">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />

	<title>Application</title>

	<!-- style -->
	<link href="css/smoothness/jquery-ui-1.10.4.custom.css" rel="stylesheet"  type="text/css" />
	<link href="css/bootstrap.min.css" rel="stylesheet"  type="text/css" />
	<link href="css/bootstrap-theme.min.css" rel="stylesheet"  type="text/css" />
	<link href="css/datepicker3.css" rel="stylesheet"  type="text/css" />
	<link href="css/app.css" rel="stylesheet"  type="text/css" />
	
</head>
<body>

    <nav class="navbar navbar-inverse navbar-static-top">
      <div class="container">
        <div class="navbar-inner">
          	<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            	<span class="icon-bar"></span>
            	<span class="icon-bar"></span>
        		<span class="icon-bar"></span>
        	</a>
			<a class="navbar-brand" href="#">Accueil</a>
        </div>
      </div>
    </nav>

    <div class="container">
		<div ng-view></div>
	  	<div>Angular seed app: v<span app-version></span></div>
	</div>

	<!-- javascript -->
	<script src="lib/validation.js"></script>
	<script src="lib/jquery-1.10.2.min.js"></script>
	<script src="lib/jquery-ui-1.10.4.custom.min.js"></script>
  	<!-- In production use:
  	<script src="//ajax.googleapis.com/ajax/libs/angularjs/x.x.x/angular.min.js"></script>
  	-->
	<script src="lib/angular/angular.js"></script>
	<script src="lib/angular/angular-route.js"></script>
	<script src="js/app.js"></script>
	<script src="js/controllers.js"></script>
	<script src="js/services.js"></script>
#foreach($entity in ${selectedEntities})
	<script src="js/${entity.name.toLowerCase()}/${entity.name.toLowerCase()}_factory.js"></script>
	<script src="js/${entity.name.toLowerCase()}/${entity.name.toLowerCase()}_service.js"></script>
	<script src="js/${entity.name.toLowerCase()}/${entity.name.toLowerCase()}_controller.js"></script>
#end
	<script src="js/filters.js"></script>
	<script src="js/directives.js"></script>
	<script src="js/datepicker.js"></script>
</body>
</html>